<h1>かけ算九九</h1>
<div id="multiplication-table"></div>

<script>
    // かけ算九九を表示する
    for (var i = 1; i <= 9; i++) {
        for (var j = 1; j <= 9; j++) {
            document.write(i + " x " + j + " = " + (i * j) + "<br>");
        }
        document.write("<br>"); // 改行を追加
    }
</script>

</body>
</html>
